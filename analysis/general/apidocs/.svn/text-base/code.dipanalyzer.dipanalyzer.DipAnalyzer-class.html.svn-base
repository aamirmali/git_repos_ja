        <span class="sig-arg">ylabel</span>=<span class="sig-default">''</span>,
        <span class="sig-arg">plot_dips</span>=<span class="sig-default">True</span>,
        <span class="sig-arg">plot_dip_ends</span>=<span class="sig-default">True</span>,
        <span class="sig-arg">plot_mins</span>=<span class="sig-default">True</span>,
        <span class="sig-arg">plot_average</span>=<span class="sig-default">False</span>,
        <span class="sig-arg">plot_spec</span>=<span class="sig-default">None</span>,
        <span class="sig-arg">plot_spec_args</span>=<span class="sig-default">None</span>)</span>
